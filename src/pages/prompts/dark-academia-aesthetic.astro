---
import BaseLayout from '../../layouts/BaseLayout.astro';
import AstroHeader from '../../components/AstroHeader.astro';
import AstroFooter from '../../components/AstroFooter.astro';
import DarkAcademiaPrompts from '../../components/DarkAcademiaPrompts';
import '../../styles/global.css';

const seo = {
  path: 'prompts/dark-academia-aesthetic',
  title: 'Dark Academia Aesthetic AI Prompts: 10 Scholarly Image Prompts | Random Prompts',
  description: 'Copy-paste dark academia AI image prompts featuring candlelit libraries, leather-bound books, autumn tones, and scholarly atmospheres. Ready for Midjourney, Stable Diffusion, and Gemini.',
  keywords: 'dark academia prompts, dark academia ai, scholarly aesthetic prompts, moody academic ai art, brown gold aesthetic, library aesthetic, midjourney dark academia',
};
---

<BaseLayout seo={seo}>
  <AstroHeader />
  <DarkAcademiaPrompts client:load />
  <AstroFooter />
</BaseLayout>
