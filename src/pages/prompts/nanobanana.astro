---
import BaseLayout from '../../layouts/BaseLayout.astro';
import AstroHeader from '../../components/AstroHeader.astro';
import AstroFooter from '../../components/AstroFooter.astro';
import NanoBananaPrompts from '../../components/NanoBananaPrompts';
import '../../styles/global.css';

const seo = {
  path: 'prompts/nanobanana',
  title: 'Nano Banana Prompts: Cat Figurines, Miniatures, and Scale Models',
  description: 'Nano Banana AI prompts for hyper-realistic cat figurines, miniature dioramas, and scale model photography. Macro photography prompts optimized for high-detail AI miniatures.',
  keywords: 'nano banana prompts, nanobanana, cat figurine prompts, miniature ai prompts, scale model prompts, macro photography ai, ceramic cat ai, diorama prompts, ai miniatures',
};
---

<BaseLayout seo={seo}>
  <AstroHeader />
  <NanoBananaPrompts client:load />
  <AstroFooter />
</BaseLayout>
