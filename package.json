{
  "name": "random-prompt-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-og-image && npm run generate-sitemap && npm run build:client && npm run build:server && npm run prerender",
